{
  "name": "hwid",
  "version": "0.4.1",
  "description": "Grab the system's unique hardware ID",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn run test && yarn run build",
    "test": "yarn run check && yarn run lint && yarn run ava",
    "check": "yarn run compile --noEmit",
    "lint": "xo",
    "clean": "rimraf lib",
    "prebuild": "yarn run clean",
    "build": "yarn run compile",
    "compile": "tsc",
    "watch": "yarn run compile --watch",
    "predev": "yarn run clean",
    "dev": "yarn run watch  --sourceMap"
  },
  "repository": "git@github.com:lolPants/hwid.git",
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/winreg": "^1.2.30",
    "ava": "^3.15.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "xo": "^0.39.1"
  },
  "dependencies": {
    "winreg": "^1.2.4"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
