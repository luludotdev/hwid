{
  "name": "hwid",
  "version": "0.6.1",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./dist/bin.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./linux": "./dist/linux.mjs",
    "./macos": "./dist/macos.mjs",
    "./windows": "./dist/windows.mjs",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "prettier --check . && eslint src test",
    "format": "prettier --write . && eslint src test --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "author": "Lulu (https://lulu.dev)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/luludotdev/hwid.git"
  },
  "devDependencies": {
    "@luludev/eslint-config": "^0.8.0",
    "@types/node": "^25.0.3",
    "@types/winreg": "^1.2.36",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "winreg": "^1.2.5"
  },
  "packageManager": "pnpm@10.26.0",
  "pnpm": {
    "overrides": {
      "@stylistic/eslint-plugin": "^5.0.0",
      "@stylistic/eslint-plugin-jsx": "npm:@stylistic/eslint-plugin",
      "@stylistic/eslint-plugin-ts": "npm:@stylistic/eslint-plugin",
      "@angular-eslint/eslint-plugin": "-",
      "@angular-eslint/eslint-plugin-template": "-",
      "@angular-eslint/template-parser": "-",
      "astro-eslint-parser": "-",
      "eslint-plugin-astro": "-",
      "eslint-plugin-cypress": "-",
      "eslint-plugin-rxjs": "-",
      "eslint-plugin-rxjs-angular": "-",
      "eslint-plugin-svelte3": "-",
      "eslint-plugin-vue": "-",
      "vue-eslint-parser": "-"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "registry-js",
      "unrs-resolver"
    ]
  }
}
