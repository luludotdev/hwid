{
  "name": "hwid",
  "version": "0.3.0",
  "description": "Grab the system's unique hardware ID",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn run test && yarn run build",
    "test": "yarn run check && yarn run lint && yarn run ava",
    "check": "yarn run compile --noEmit",
    "lint": "xo",
    "clean": "rimraf lib esm",
    "prebuild": "yarn run clean",
    "build:cjs": "ncc build ./src/index.ts -o lib -m -e winreg -e tslib",
    "build:esm": "yarn run compile --module ES6 --outDir esm",
    "build": "yarn run build:cjs && yarn run build:esm",
    "compile": "tsc",
    "watch": "yarn run compile --watch",
    "predev": "yarn run clean",
    "dev": "yarn run watch  --sourceMap"
  },
  "repository": "git@github.com:lolPants/hwid.git",
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/winreg": "^1.2.30",
    "@vercel/ncc": "^0.27.0",
    "ava": "^3.15.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "xo": "^0.38.2"
  },
  "dependencies": {
    "tslib": "^2.1.0",
    "winreg": "^1.2.4"
  },
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
